  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "\"node_modules/.pnpm/(?!react-use-localstorage)/\"",
      "\"node_modules/.pnpm/(?!(monaco-editor|monaco-editor-core)/)\""
    ]
  }
